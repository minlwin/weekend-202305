{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "hello-component": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineStyle": true,
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/hello-component",
      "sourceRoot": "projects/hello-component/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/hello-component",
            "index": "projects/hello-component/src/index.html",
            "browser": "projects/hello-component/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/hello-component/tsconfig.app.json",
            "assets": [
              "projects/hello-component/src/favicon.ico",
              "projects/hello-component/src/assets"
            ],
            "styles": [
              "projects/hello-component/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "hello-component:build:production"
            },
            "development": {
              "buildTarget": "hello-component:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "hello-component:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/hello-component/tsconfig.spec.json",
            "assets": [
              "projects/hello-component/src/favicon.ico",
              "projects/hello-component/src/assets"
            ],
            "styles": [
              "projects/hello-component/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "data-binding": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineStyle": true,
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/data-binding",
      "sourceRoot": "projects/data-binding/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/data-binding",
            "index": "projects/data-binding/src/index.html",
            "browser": "projects/data-binding/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/data-binding/tsconfig.app.json",
            "assets": [
              "projects/data-binding/src/favicon.ico",
              "projects/data-binding/src/assets"
            ],
            "styles": [
              "projects/data-binding/src/styles.css"
            ],
            "scripts": [],
            "server": "projects/data-binding/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "projects/data-binding/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "data-binding:build:production"
            },
            "development": {
              "buildTarget": "data-binding:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "data-binding:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/data-binding/tsconfig.spec.json",
            "assets": [
              "projects/data-binding/src/favicon.ico",
              "projects/data-binding/src/assets"
            ],
            "styles": [
              "projects/data-binding/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "a61e4eb3-a79e-42b3-ac84-81ff7cfc72a6"
  }
}
